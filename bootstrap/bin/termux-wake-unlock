#!/data/data/pl.sviete.dom/files/usr/bin/sh

if [ $# != 0 ]; then
	echo 'usage: termux-wake-unlock'
	echo 'Release the Termux wake lock to allow the CPU to sleep.'
	exit 1
fi

am startservice \
	--user 0 \
	-a pl.sviete.termux.service_wake_unlock \
	pl.sviete.dom/pl.sviete.termux.app.TermuxService \
	> /dev/null
