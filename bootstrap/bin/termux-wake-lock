#!/data/data/pl.sviete.dom/files/usr/bin/sh

if [ $# != 0 ]; then
	echo 'usage: termux-wake-lock'
	echo 'Acquire the Termux wake lock to prevent the CPU from sleeping.'
	exit 1
fi

am startservice \
	--user 0 \
	-a pl.sviete.termux.service_wake_lock \
	pl.sviete.dom/pl.sviete.termux.app.TermuxService \
	> /dev/null
